{
  "name": "jwt-auth",
  "version": "1.0.0",
  "description": "A robust set of tools and features designed to streamline business administration.",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "minify",
        {
          "mangle": {
            "exclude": [
              "ParserError",
              "NetworkError",
              "TypeError"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "minify-type-constructors",
      "transform-minify-booleans"
    ]
  },
  "scripts": {
    "build": "babel src --out-dir build",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --exec babel-node",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilfuegosDEV/JWT-AUTH.git"
  },
  "keywords": [
    "dashboard",
    "dinocorp",
    "business",
    "administration"
  ],
  "author": "MilfuegosDEV",
  "bugs": {
    "url": "https://github.com/MilfuegosDEV/JWT-AUTH/issues"
  },
  "homepage": "https://github.com/MilfuegosDEV/JWT-AUTH#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.7",
    "babel-plugin-minify-type-constructors": "^0.4.3",
    "babel-plugin-transform-minify-booleans": "^6.9.4",
    "babel-preset-minify": "^0.5.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0"
  }
}
